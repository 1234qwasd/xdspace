<%
    if(typeof preload == 'string')
        preload = [preload];
%>
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title><%= config.SERVER_NAME %></title>
    <meta name="description" content="<%= config.SERVER_DESCRIPTION %>" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no">
    <!-- <meta property="og:title" content="<%= config.SERVER_NAME %>"/> -->
    <meta property="og:determiner" content="<%= config.SERVER_NAME.toUpperCase().substring(0, 2) %>"/>
    <meta property="og:type" content="website"/>
    <meta property="og:description" content="<%= config.SERVER_DESCRIPTION %>"/>
    <meta property="og:image" content="<%= origin %>/u/1/avatar">
    <meta property="og:url" content="<%= origin %>/?id=1">
    <meta property="og:site_name" content="<%= new URL(origin).hostname.toUpperCase() %>"/>
    <link rel="icon" type="image/png" href="/favicon.png" />
    <% if(typeof noDefaultStyle === 'undefined' || !noDefaultStyle) { %>
    <link rel="stylesheet" href="/style.css">
    <% } %>

    <% if(typeof jquery !== 'undefined' && jquery) { %>
    <script type="text/javascript" src="/jquery.js"></script>
    <% } %>

    <% if(typeof css != 'undefined') { %>
    <style><%= css %></style>
    <% } %>

    <% if(typeof preload != 'undefined') { %>
    <% for(let link of preload.filter(o => o)) { %>
    <link rel="preload" href="<%= link %>" as="style">
    <link rel="stylesheet" type="text/css" href="<%= link %>">
    <% } %>
    <% } %>

    <% if(viewer && viewer.admin) { %>
    <link rel="stylesheet" type="text/css" href="/admin.css">
    <% } %>
</head>
